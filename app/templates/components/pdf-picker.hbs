<h1>Generate csv</h1>
{{#if file}}
  <a href="#" {{action "parsePdf"}}>Parse {{file}}</a>
{{else}}
  <a href="#" {{action "pickFile"}}>Choose file</a>
{{/if}}

{{#if rows}}
  <table>
    <tbody>
      {{#each rows as |row|}}
        <tr>
           {{#each-in row as |k v|}}
              <td>{{v}}</td>
            {{/each-in}}
        </tr>
      {{/each}}
    </tbody>
  </table>
{{/if}}